{
  "name": "eink-dashboard",
  "version": "2.0.0",
  "repository": "git@github.com:stephenyu/eink-dashboard.git",
  "author": "Stephen Yu <s.yu@less3.com>",
  "license": "GPL-3.0-or-later",
  "watch": {
    "dev_build": {
      "patterns": [
        "src"
      ],
      "extensions": "tsx,html",
      "quiet": false
    },
    "dev_web": {
      "patterns": [
        "src"
      ],
      "extensions": "tsx,html",
      "quiet": false
    }
  },
  "scripts": {
    "dev": "npm-watch dev_build",
    "dev-web": "npm-watch dev_web",
    "start": "NODE_PATH=\"$(pwd)/src:${NODE_PATH:-}\" sh ./bin/build.sh prod",
    "dev_build": "NODE_PATH=\"$(pwd)/src:${NODE_PATH:-}\" sh ./bin/build.sh dev",
    "dev_web": "NODE_PATH=\"$(pwd)/src:${NODE_PATH:-}\" ts-node dev_mode.ts"
  },
  "dependencies": {
    "@types/react": "^17.0.16",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.12",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.25.1",
    "npm-watch": "^0.11.0",
    "puppeteer": "^10.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.0",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.0",
    "typescript": "^4.4.2"
  },
  "devDependencies": {
    "@types/node": "^16.7.13",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0"
  }
}
